{
	"name": "vscode-mydemo",
	"displayName": "vscode-mydemo",
	"description": "vscode-mydemo",
	"version": "0.0.1",
	"keywords": [
		"kaiyi"
	],
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-mydemo.helloWorld",
		"onCommand:vscode-mydemo.aaaaaaaaaaaaaa",
		"onCommand:vscode-mydemo.bbbbbbbbbbbbbb",
		"onLanguage:json",
		"onLanguage:javascript"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-mydemo.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "vscode-mydemo.aaaaaaaaaaaaaa",
				"title": "aaaaaaaaaaaaaa"
			},
			{
				"command": "vscode-mydemo.bbbbbbbbbbbbbb",
				"title": "bbbbbbbbbbbbbb"
			},
			{
				"command": "vscode-mydemo.cccccccccccccc",
				"title": "cccccccccccccc"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "vscode-mydemo.helloWorld",
					"group": "navigation@1"
				},
				{
					"when": "editorFocus",
					"command": "vscode-mydemo.aaaaaaaaaaaaaa",
					"group": "navigation@1"
				},
				{
					"when": "editorFocus",
					"command": "vscode-mydemo.bbbbbbbbbbbbbb",
					"group": "navigation@1"
				},
				{
					"when": "editorFocus",
					"command": "vscode-mydemo.cccccccccccccc",
					"group": "navigation@1"
				}
			],
			"explorer/context": [
				{
					"command": "vscode-mydemo.aaaaaaaaaaaaaa",
					"group": "navigation@1"
				},
				{
					"command": "vscode-mydemo.bbbbbbbbbbbbbb",
					"group": "navigation@1"
				}
			]
		},
		"configuration": {
			"title": "KaiyiVscodeDemo",
			"properties": {
				"KaiyiVscodeDemo.aaaaaaaaaaaaaa": {
					"type": "boolean",
					"default": false,
					"description": "kaiyi-test-description，其实是checkbox的label"
				},
				"KaiyiVscodeDemo.bbbbbbbbbbbbbb": {
					"type": [
						"string",
						"null"
					],
					"default": null,
					"markdownDescription": "这是markdown格式的例子，`代码块`，[百度](https://www.baidu.com) "
				},
				"KaiyiVscodeDemo.dddddddddddddd": {
					"type": "string",
					"default": "吃了吗",
					"enum": [
						"吃了吗",
						"吃了"
					],
					"enumDescriptions": [
						"enumDescriptions第一个字段",
						"enumDescriptions第二个字段"
					]
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch --stats-error-details",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"ts-loader": "^8.0.14",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}